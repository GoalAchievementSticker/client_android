<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.java_sticker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Java_Sticker">
        <activity
            android:theme="@style/MyThemeNoActionBar"
            android:name=".Group_main"
            android:exported="true" />
        <activity
            android:theme="@style/MyThemeNoActionBar"
            android:name=".mypage"
            android:exported="true" />
        <activity
            android:theme="@style/MyThemeNoActionBar"
            android:name=".Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Signup"
            android:theme="@style/MyThemeNoActionBar"
            android:exported="true" />
        <activity
            android:name=".custom_p_goal_click"
            android:theme="@style/MyThemeNoActionBar"
            android:exported="true" />
        <activity
            android:name=".custom_g_goal_click"
            android:theme="@style/MyThemeNoActionBar"
            android:exported="true" />
        <activity android:name=".gGoalInput.First"
            android:theme="@style/MyThemeNoActionBar"
            android:exported="true" />
        <activity android:name=".gGoalInput.Second"
            android:theme="@style/MyThemeNoActionBar"
            android:exported="true" />
        <activity android:name=".gGoalInput.Third"
            android:theme="@style/MyThemeNoActionBar"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/MyThemeNoActionBar"
            android:exported="true" />
    </application>

</manifest>